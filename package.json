{
  "name": "@yandex-data-ui/page-constructor",
  "version": "1.1.0",
  "description": "Cloud Page Constructor",
  "author": "DataUI Team <data-ui@yandex-team.ru>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@github.yandex-team.ru:data-ui/page-constructor.git"
  },
  "files": [
    "build",
    "styles",
    "server"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "typings": "build/esm/index.d.ts",
  "scripts": {
    "deps:install": "npm ci",
    "deps:truncate": "npm prune --production",
    "lint:fix": "run-s lint:js:fix lint:styles:fix lint:prettier:fix typecheck",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}' --max-warnings=0",
    "lint:js:fix": "eslint '**/*.{js,jsx,ts,tsx}' --max-warnings=0 --quiet --fix",
    "lint:styles": "stylelint '{styles,src}/**/*.scss' -s scss",
    "lint:styles:fix": "stylelint **/*.scss -s scss --fix",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}'",
    "lint": "run-p lint:js lint:styles lint:prettier typecheck",
    "typecheck": "tsc --noEmit",
    "dev": "npm run storybook:start",
    "storybook:start": "TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 7009",
    "storybook:build": "TS_NODE_PROJECT=.storybook/tsconfig.json build-storybook -c .storybook -o storybook-static",
    "start": "node dist",
    "clean": "gulp clean",
    "build:client": "gulp",
    "build:server": "rimraf server && tsc -p tsconfig.server.json && move-file server/server.js server/index.js && move-file server/server.d.ts server/index.d.ts",
    "build": "run-p build:client build:server",
    "docker:build": "docker build . -t registry.yandex.net/data-ui/page-constructor:${npm_package_version}",
    "docker:push": "docker push registry.yandex.net/data-ui/page-constructor:${npm_package_version}",
    "docker:release": "ya tool dctl release docker --release-type stable -t ${npm_package_version} data-ui/page-constructor",
    "prepublishOnly": "npm run lint && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "bem-cn-lite": "^4.0.0",
    "lodash": "^4.17.21",
    "react-player": "^2.9.0",
    "react-slick": "^0.28.1",
    "react-spring": "^9.3.0",
    "react-transition-group": "^4.4.2",
    "react-waypoint": "^10.1.0",
    "sanitize-html": "^2.6.1",
    "snakecase-keys": "^5.1.0",
    "typograf": "^6.14.0"
  },
  "peerDependencies": {
    "@yandex-cloud/uikit": "^2.12.0",
    "@yandex-cloud/i18n": "^0.6.0",
    "@doc-tools/transform": "^2.6.1",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@doc-tools/transform": "^2.12.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.10",
    "@types/lodash": "^4.14.176",
    "@types/react": "^16.14.20",
    "@types/react-slick": "^0.23.7",
    "@types/react-transition-group": "^4.4.4",
    "@types/sanitize-html": "^2.6.0",
    "@yandex-cloud/eslint-config": "^1.0.1",
    "@yandex-cloud/i18n": "^0.6.0",
    "@yandex-cloud/prettier-config": "^1.0.0",
    "@yandex-cloud/stylelint-config": "^1.1.1",
    "@yandex-cloud/tsconfig": "^1.0.0",
    "@yandex-cloud/uikit": "^2.12.0",
    "eslint": "^7.32.0",
    "eslint-plugin-local": "./eslint-plugin-local",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-loader": "^1.3.2",
    "husky": "^7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.2.6",
    "markdown-loader": "^6.0.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.4.1",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.4",
    "sass-loader": "^10.3.1",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.21.0",
    "svg-sprite-loader": "^6.0.11",
    "tslib": "^2.4.0",
    "typescript": "4.4.4",
    "webpack": "^4.46.0"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,yaml,yml,md}": [
      "prettier --write"
    ]
  },
  "preview-generator": {
    "prepare": [
      "npm i @yandex-data-ui/core@3.83.0",
      "npm run storybook:build",
      "npx sb extract",
      "touch storybook-static/metadata.json",
      "rm -rf dist/public",
      "mv storybook-static dist/public",
      "mkdir -p dist/run"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "publishConfig": {
    "tag": "alpha"
  }
}
